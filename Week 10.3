n = int(input().strip())
A = list(map(int, input().strip().split()))
peaks = []
if n > 0 and (n == 1 or A[0] >= A[1]):
    peaks.append(A[0])
for i in range(1, n - 1):
    if A[i - 1] <= A[i] >= A[i + 1]:
        peaks.append(A[i])
if n > 1 and A[n - 1] >= A[n - 2]:
    peaks.append(A[n - 1])
print(" ".join(map(str, peaks)))
